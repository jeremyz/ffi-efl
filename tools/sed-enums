s/^\s*typedef\s+(enum(\s+\w+)?).*$/typedef \1 { /
T;h
:attrs;n
s/^\s*([A-Z0-9_]+)(\s*=\s*-?[0-9]+)?.*$/\1\2, /
T next;H;b attrs
:next
/^\s*\}/ ! b attrs
s/^\s*}\s*(\w+).*$/ } \1;/
t finish
:close
s/^\s*typedef\s+enum\s+\w+\s+(\w+)\s+;/ } \1/
t finish;n;T close
:finish
H;x
s/\n//g
s/ {2,}/ /g
s/, }/ }/p
